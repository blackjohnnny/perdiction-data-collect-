# BSC RPC endpoint (default: https://bsc-dataseed.binance.org)
# You can use other BSC RPC providers like:
# - https://bsc-dataseed1.binance.org
# - https://bsc-dataseed2.binance.org
# - https://rpc.ankr.com/bsc
# - https://bsc.publicnode.com
BSC_RPC=https://bsc-dataseed.binance.org

# Polling interval for live mode in milliseconds (default: 3000)
POLL_INTERVAL_MS=3000

# Database file path (default: ./prediction-data.db)
DB_PATH=./prediction-data.db

# Backfill concurrency (default: 6)
CONCURRENCY=6

# Maximum retries for failed RPC calls (default: 5)
MAX_RETRIES=5

# Base delay for retry backoff in milliseconds (default: 200)
RETRY_BASE_DELAY_MS=200
